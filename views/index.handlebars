{{#each articles}}
<section>
    <article>
        <h1>{{title}}</h1>
        <p>{{timestamp}}</p>
        <div class="row">
            <div class="col-md-8 article-content">
                {{{preview}}}
            </div>
        </div>
    </article>
</section>
{{/each}}
<script>
(function() {
    $('.article-content blockquote').addClass('pull-right');
})();
</script>